{
  "name": "vanta/gitlab-rector",
  "description": "Formatter artifacts rector for gitlab",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Vlad Shashkov",
      "email": "v.shashkov@pos-credit.ru"
    },
    {
      "name": "Vanta Team",
      "email": "info@vanta.ru"
    }
  ],
  "keywords": [
    "gitlab",
    "rector",
    "artifacts"
  ],
  "autoload": {
    "psr-4": {
      "Vanta\\Integration\\Rector\\": "src/"
    }
  },
  "require": {
    "php": "^8.2",
    "rector/rector": "^1.0.1",
    "nette/utils": "^4.0",
    "violet/streaming-json-encoder": "^1.1"
  },
  "require-dev": {
    "symfony/var-dumper": "^6.3",
    "phpstan/phpstan": "^1.10",
    "spaze/phpstan-disallowed-calls": "^2.15",
    "ergebnis/phpstan-rules": "^2.0",
    "phpstan/extension-installer": "^1.3",
    "friendsofphp/php-cs-fixer": "^3.22"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
